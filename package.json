{
  "name": "messenger",
  "version": "1.0.0",
  "description": "",
  "targets": {
    "main": false
  },
  "scripts": {
    "build": "parcel build index.html",
    "dev": "parcel index.html --no-cache --port 3000 --open",
    "eslint": "eslint src --ext .ts,.js --fix",
    "test": "jest",
    "stylelint": "stylelint src/**/*.scss --fix",
    "start": "yarn build && node ./server.js",
    "generate-icons": "yarn run fantasticon"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "helpers": "./src/helpers",
    "pages": "./src/pages"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost:3000",
    "setupFiles": [
      "<rootDir>/src/tests/setupEnv.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|svg|scss)": "identity-obj-proxy",
      "^core(.*)$": "<rootDir>/src/core$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^helpers(.*)$": "<rootDir>/src/helpers$1",
      "^pages(.*)$": "<rootDir>/src/pages$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rnuravtsev/messenger.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rnuravtsev/messenger/issues"
  },
  "homepage": "https://github.com/rnuravtsev/messenger#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.21.0",
    "@parcel/transformer-sass": "2.8.2",
    "@testing-library/dom": "9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@types/chai": "4.3.4",
    "@types/dotenv": "8.2.0",
    "@types/hbs": "4.0.1",
    "@types/jest": "27.5.2",
    "@types/mocha": "10.0.1",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "assert": "^2.0.0",
    "buffer": "^5.7.1",
    "chai": "4.3.7",
    "eslint": "8.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "fantasticon": "2.0.0",
    "handlebars": "4.7.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "mocha": "10.2.0",
    "msw": "1.1.0",
    "parcel": "2.8.2",
    "parcel-plugin-handlebars": "0.4.4",
    "path-browserify": "^1.0.1",
    "postcss-scss": "4.0.6",
    "punycode": "^1.4.1",
    "sass": "1.56.0",
    "sinon": "15.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "stylelint": "14.15.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-scss": "4.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "util": "^0.12.5",
    "whatwg-fetch": "3.6.2"
  },
  "dependencies": {
    "express": "4.18.2",
    "nanoid": "3.3.4",
    "normalize.css": "8.0.1",
    "path": "0.12.7",
    "uuid": "9.0.0"
  }
}
