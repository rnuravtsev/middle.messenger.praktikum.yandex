{
  "name": "messenger",
  "version": "1.0.0",
  "description": "",
  "targets": {
    "main": false
  },
  "scripts": {
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "build": "webpack --mode=production --node-env=production",
    "serve": "webpack serve",
    "watch": "webpack --watch",
    "start": "npm run generate-icons && npm run build && node ./server.js",
    "eslint": "eslint src --ext .ts,.js --fix",
    "stylelint": "stylelint src/**/*.scss --fix",
    "test:static": "npm run eslint && npm run stylelint",
    "test": "jest",
    "generate-icons": "fantasticon",
    "prepare": "husky install"
  },
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "helpers": "./src/helpers",
    "pages": "./src/pages"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost:3000",
    "setupFiles": [
      "<rootDir>/src/tests/setupEnv.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|svg|scss)": "identity-obj-proxy",
      "^core(.*)$": "<rootDir>/src/core$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^helpers(.*)$": "<rootDir>/src/helpers$1",
      "^pages(.*)$": "<rootDir>/src/pages$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rnuravtsev/messenger.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rnuravtsev/messenger/issues"
  },
  "homepage": "https://github.com/rnuravtsev/messenger#readme",
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.21.0",
    "@testing-library/dom": "9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@types/chai": "4.3.4",
    "@types/dotenv": "8.2.0",
    "@types/hbs": "4.0.1",
    "@types/jest": "27.5.2",
    "@types/mocha": "10.0.1",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "assert": "2.0.0",
    "buffer": "5.7.1",
    "chai": "4.3.7",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "eslint": "8.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "fantasticon": "2.0.0",
    "handlebars": "4.7.7",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "mini-css-extract-plugin": "2.7.3",
    "mocha": "10.2.0",
    "msw": "1.1.0",
    "path-browserify": "1.0.1",
    "postcss-scss": "4.0.6",
    "proxyquire": "2.1.3",
    "punycode": "1.4.1",
    "sass": "1.58.3",
    "sass-loader": "13.2.0",
    "sinon": "15.0.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "string_decoder": "1.3.0",
    "style-loader": "3.3.1",
    "stylelint": "14.15.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-scss": "4.3.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "util": "0.12.5",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "whatwg-fetch": "3.6.2"
  },
  "dependencies": {
    "express": "4.18.2",
    "nanoid": "3.3.4",
    "normalize.css": "8.0.1",
    "path": "0.12.7",
    "uuid": "9.0.0"
  }
}
